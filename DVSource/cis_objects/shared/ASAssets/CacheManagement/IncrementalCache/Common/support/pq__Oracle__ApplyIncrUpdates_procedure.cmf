  <attribute name="SQL" type="STRING">&lt;version 2&gt; multipartseparator=;+;
MERGE INTO 
  &quot;{0}&quot; c
USING (
  SELECT 
{1}
  FROM 
    &quot;{2}&quot;
  ORDER BY
    &quot;{3}&quot;
) d
ON ({4})
WHEN MATCHED THEN
  UPDATE SET 
{5}
  DELETE WHERE (d.&quot;{3}&quot; = {6})
WHEN NOT MATCHED THEN
  INSERT (
{7}
  ) VALUES (
{8}
  ); ;
COMMIT; ;
